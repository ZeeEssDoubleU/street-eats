{
  "kind": "collectionType",
  "collectionName": "dishes",
  "info": {
    "name": "dish"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "restaurant": {
      "via": "dishes",
      "model": "restaurant"
    }
  }
}
