{
	"name": "street-eats-v2",
	"version": "1.0.0",
	"description": "Street Eats.  A food delivery app.",
	"main": "index.js",
	"scripts": {
		"client": "yarn --cwd client start",
		"client-dev": "yarn --cwd client dev",
		"client-build": "yarn --cwd client build",
		"client-i": "yarn --cwd client install",
		"client-add": "yarn --cwd client add",
		"client-upgrade": "yarn --cwd client upgrade",
		"client-remove": "yarn --cwd client remove",
		"client-clean": "yarn --cwd client clean",
		"client-env": "yarn --cwd client env",
		"server": "yarn --cwd server start",
		"server-dev": "yarn --cwd server dev",
		"server-build": "yarn --cwd server build",
		"server-i": "yarn --cwd server install",
		"server-add": "yarn --cwd server add",
		"server-upgrade": "yarn --cwd server upgrade",
		"server-remove": "yarn --cwd server remove",
		"server-clean": "yarn --cwd server clean",
		"all-i": "yarn install && yarn client-i && yarn server-i",
		"all-upgrade": "yarn upgrade && yarn client-upgrade && yarn server-upgrade",
		"all-clean": "yarn clean && yarn client-clean && yarn server-clean",
		"start": "concurrently 'yarn client' 'yarn server'",
		"dev": "concurrently 'yarn client-dev' 'yarn server-dev'",
		"build": "yarn server-build && concurrently 'yarn server' 'yarn client-build'",
		"heroku-postbuild": "yarn all-add && yarn build",
		"clean": "rm -rf node_modules && rm yarn.lock",
		"test": "echo \"Error: no test specified\" && exit 1",
		"env-dupe": "cp .env ./server/.env && cp .env ./client/.env"
	},
	"keywords": [],
	"author": "Zachary Williams <zak.williams2287@gmail.com>",
	"license": "MIT",
	"devDependencies": {},
	"dependencies": {
		"concurrently": "^6.0.2",
		"dotenv": "^8.2.0"
	}
}
